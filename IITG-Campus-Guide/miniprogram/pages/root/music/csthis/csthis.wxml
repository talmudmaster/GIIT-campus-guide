            <view bindtap="a" data-id="{{item._id}}" style="margin-left:50rpx;width:80vw">歌曲名: <text decode="{{true}}"> &ensp;&ensp;{{item.songname.ipuname3}}</text></view>
            <view bindtap="a" data-id="{{item._id}}" style="margin-left:50rpx;width:80vw">歌曲名: <text decode="{{true}}"> &ensp;&ensp;{{item.songname.ipuname4}}</text></view>
