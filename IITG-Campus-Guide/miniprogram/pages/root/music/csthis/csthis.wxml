<!--miniprogram/pages/root/music/csthis/csthis.wxml-->
<view>
    <view style="margin-top:50rpx"></view>
    <view style="text-align:center">推荐的全部信息</view>
    <view style="margin-top:90rpx"></view>
    <view wx:for="{{list}}" wx:key="key">
        <view>
            <view bindtap="a" data-id="{{item._id}}" style="margin-left:50rpx;width:80vw">歌曲名: <text decode="{{true}}"> &ensp;&ensp;{{item.songname.ipuname0}}</text></view>
